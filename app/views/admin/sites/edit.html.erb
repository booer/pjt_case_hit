<%= render 'admin/common/header' %>
<section>
  <%= render 'admin/common/leftpanel' %>

  <div class="mainpanel">
    <div class="contentpanel">

      <ol class="breadcrumb breadcrumb-quirk">
        <li><i class="fa fa-home mr5"></i> Home</li>
        <li class="active">站台設定</li>
      </ol>

      <%= bootstrap_form_for [:admin, @site] do |e| %>
        <%= e.text_field :fax %>
        <%= e.text_field :tel %>
